    <string name="wallet_faq_questions_question_3">"Où mes données sont-elles stockées?"</string>

    <!-- Frage 4 im FAQ-Screen der Wallet App -->
    <string name="wallet_faq_questions_question_4">"Comment prévient-on les abus et les falsifications?"</string>

    <!-- Frage 5 im FAQ-Screen der Wallet App -->
    <string name="wallet_faq_questions_question_5">"Et si je perds mon certificat COVID?"</string>

    <!-- Frage 6 im FAQ-Screen der Wallet App -->
    <string name="wallet_faq_works_question_1">"Comment ajouter un certificat COVID à l'app?"</string>

    <!-- Frage 7 im FAQ-Screen der Wallet App -->
    <string name="wallet_faq_works_question_2">"Puis-je ajouter plusieurs certificats COVID?"</string>

    <!-- Frage 8 im FAQ-Screen der Wallet App -->
    <string name="wallet_faq_works_question_3">"Comment savoir si mon certificat est valable?"</string>

    <!-- Frage 9 im FAQ-Screen der Wallet App -->
