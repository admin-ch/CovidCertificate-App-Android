    <!-- Titel 1 im FAQ-Screen der Wallet App -->
    <string name="wallet_faq_questions_title">"Le certificat COVID en bref"</string>

    <!-- Text 1im FAQ-Screen der Wallet App -->
    <string name="wallet_faq_questions_subtitle">"Le certificat COVID atteste de manière infalsifiable une vaccination contre le COVID-19, une guérison ou un résultat de test négatif."</string>

    <!-- Frage 1 im FAQ-Screen der Wallet App -->
    <string name="wallet_faq_questions_question_1">"Quand et où puis-je obtenir un certificat COVID?"</string>

    <!-- Frage 2 im FAQ-Screen der Wallet App -->
    <string name="wallet_faq_questions_question_2">"Par quel moyen puis-je présenter un certificat COVID?"</string>

